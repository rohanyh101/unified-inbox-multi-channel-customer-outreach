{
  "name": "attack_capital",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:full": "concurrently \"npm run dev\" \"node websocket-server.js\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset",
    "db:reset-seed": "prisma db push --force-reset && npm run db:seed",
    "db:migrate-init": "prisma migrate dev --name init",
    "setup-admin": "tsx scripts/setup-admin.ts",
    "demote-admin": "tsx scripts/demote-admin.ts",
    "scheduler:run": "curl -X POST http://localhost:3000/api/cron/process-scheduled-messages -H 'Authorization: Bearer dev-secret-key'",
    "ws:dev": "node websocket-server.js"
